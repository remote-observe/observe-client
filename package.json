{
  "name": "observe-client",
  "description": "CLI client for interfacing with an observatory and communicating with a firebase instance",
  "version": "0.0.0",
  "author": "Bret Little @blittle",
  "bin": {
    "observe-client": "./bin/run"
  },
  "bugs": "https://github.com/remote-observe/observe-client/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "firebase-admin": "^7.0.0",
    "lodash": "^4.17.11",
    "observe-weather": "0.0.1",
    "systemjs": "^3.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "globby": "^8",
    "husky": "^1.3.1",
    "mocha": "^5",
    "npm": "^6.8.0",
    "nyc": "^13",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/remote-observe/observe-client",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "observe-client",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "remote-observe/observe-client",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && pretty-quick --staged"
    }
  }
}
